{
  "name": "apc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001 --strictPort",
    "build": "tsc && vite build --mode production",
    "build:watch": "tsc && vite build --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --port 3001 --strictPort",
    "serve": "rm -rf .dist && concurrently \"pnpm build:watch\" \"pnpm preview\""
  },
  "dependencies": {
    "ag-grid-community": "^31.1.1",
    "ag-grid-react": "^31.1.1",
    "antd": "^5.15.2",
    "concurrently": "^8.2.2",
    "i18next": "^23.8.2",
    "jotai": "^2.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.3.4",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
